        "bad-words": "^3.0.4",
        "bullmq": "^5.12.8",
        "chrono-node": "^2.7.6",
        "dotenv": "^16.4.5",
        "ioredis": "^5.4.1",
        "logger": "^1.0.6",
        "luxon": "^3.5.0",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45",
        "naughty-words": "^1.2.0",
        "node-schedule": "^2.1.1",
        "numeral": "^2.0.6",
